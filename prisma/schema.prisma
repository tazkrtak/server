datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
 
model User {
  id            String    @id
  email         String    @unique
  phone_number  String    @unique
  full_name     String
  password      String
  credit        Float     @default(0)
  secret        String
  created_at    DateTime  @default(now())
}